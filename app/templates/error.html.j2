{% extends "layout.html.j2" %}
{% block meta_title %}Error{% endblock meta_title %}
{% block meta_description %}Error: {{ message | default('Hmmm... the page you are looking for can not be found.') }}{% endblock %}
{% block meta_url %}{{ url_for('error', _external=True) }}{% endblock meta_url %}">
{% block title %}{{ title | default('Page Not Found') }}{% endblock %}
{% block header %}{{ title | default('Page Not Found') }}{% endblock %}
{%- block content %}
                <p>
                    {{ message | default("Hmmm... The page you are looking for can not be found.") }}
                </p>
                <p>
                    You may want to return to the <a href="{{ url_for('welcome') }}">front page</a>.
                </p>
{%- endblock %}
